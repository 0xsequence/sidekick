{
  "name": "sequence_sidekick",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "start:withRedis": "redis-server --daemonize yes && tsx src/index.ts",
    "dev:withRedis": "redis-server --daemonize yes && tsx watch src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "docker:restart": "docker compose down && docker compose up -d",
    "docker:stop": "docker compose down -v",
    "docker:start": "docker compose up -d",
    "stop:redis": "redis-server stop",
    "start:redis": "redis-server --daemonize yes"
  },
  "devDependencies": {
    "@types/bull-board": "^2.0.4",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "3.0.5",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@0xsequence/auth": "^2.2.11",
    "@0xsequence/google-kms-signer": "^0.2.0",
    "@0xsequence/indexer": "^2.2.13",
    "@0xsequence/network": "^2.2.11",
    "@aws-sdk/client-kms": "^3.750.0",
    "@bull-board/api": "^6.7.8",
    "@bull-board/fastify": "^6.7.8",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/properties": "^5.7.0",
    "@ethersproject/signing-key": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@fastify/cors": "^10.0.2",
    "@fastify/redis": "^7.0.2",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.1",
    "@google-cloud/kms": "^4.5.0",
    "@peculiar/asn1-ecc": "^2.3.15",
    "@peculiar/asn1-schema": "^2.3.15",
    "@peculiar/asn1-x509": "^2.3.15",
    "@prisma/client": "^6.3.1",
    "@sinclair/typebox": "^0.34.16",
    "@types/bull": "^4.10.4",
    "abitype": "^1.0.8",
    "asn1js": "^3.0.5",
    "bull": "^4.16.5",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "pkijs": "^3.2.4",
    "prisma": "^6.3.1",
    "viem": "^2.22.23",
    "vitest": "^3.0.5"
  }
}